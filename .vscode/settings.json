{
    "workbench.colorCustomizations": {
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveForeground": "#15202b99",
        "titleBar.activeBackground": "#556788",
        "titleBar.inactiveBackground": "#556788",
        "activityBar.activeBackground": "#556788",
        "activityBar.activeBorder": "#009f7b",
        "activityBar.background": "#556788",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#009f7b",
        "activityBarBadge.foreground": "#e7e7e7",
        "statusBar.background": "#556788",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#556788"
    },
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false
    },
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "prettier.disableLanguages": [
        "javascript",
        "javascriptreact"
    ],
    "peacock.color": "#FFC600"
}